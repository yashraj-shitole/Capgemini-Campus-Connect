package Hash_Map;

import java.util.HashMap;
import java.util.LinkedHashMap;

public class Hash_Map_Code {

	public static void main(String[] args) {

		
		//maintain unique ID
		//Order not maintained
		//can have duplicate data
	
		HashMap<Long, String> hashMap=new HashMap<>();
		
		hashMap.put(7219014381l, "asd");
		hashMap.put(9876543210l, "zxcv");
		hashMap.put(8765432109l, "qwe");
		hashMap.put(7654321098l, "fghvb");
		hashMap.put(6543210987l, "jklv");
		hashMap.put(5432109876l, "tyu");
		hashMap.put(4321098765l, "zxcvc");
		
		
		System.out.println(hashMap);
		
		
		LinkedHashMap<Long, String> linkedHashMap=new LinkedHashMap<>();
		
		linkedHashMap.put(7219014381l, "asd");
		linkedHashMap.put(9876543210l, "zxcv");
		linkedHashMap.put(8765432109l, "qwe");
		linkedHashMap.put(7654321098l, "fghvb");
		linkedHashMap.put(6543210987l, "jklv");
		linkedHashMap.put(5432109876l, "tyu");
		linkedHashMap.put(4321098765l, "zxcvc");
		
		System.out.println(linkedHashMap);

	}

}
