import java.util.Iterator;

public class ArrayEvenOdd {
	
	public static void main(String args[]) {
		
		
		int[] a= {1,2,3,4,5};
		int[] even=new int[2];
		int[] odd=new int[3];
		
		for (int i : a) {
			
			if(i%2==0) {
				
				for(int j=0;j<even.length;j++) {
					even[j]=a[i];
				}
			}
			else {
				for(int z=0;z<odd.length;z++) {
					odd[z]=a[i];
				}
			}
		}
		
		for (int i : odd) {
			System.out.println(odd[i]);
		}
		
		for (int i : even) {
			System.out.println(even[i]);
			
		}
		
	}

}
