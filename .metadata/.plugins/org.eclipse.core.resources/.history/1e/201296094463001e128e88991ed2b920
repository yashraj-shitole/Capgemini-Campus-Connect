package com.testing;

public class Primenumber {
	
	public static void main(String args[]) {
		
		int n=19;
		int c=0;
		for(int i=2;i<n;i++) {
			
			if(n%2==0) {
				c++;
				break;
			}
		}
		
		if(c>0) {
			System.out.println("is not prime");
		}
		else {
			System.out.println("prime");
		}
	}

}
