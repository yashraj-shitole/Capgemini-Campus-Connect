import java.util.Scanner;

public class Factorial {
	
	public static void main(String args[]) {
		
		Scanner scanner=new Scanner(System.in);
		long num=scanner.nextLong();
		scanner.close();
		int factorial=1;
		
		
		for(int i=1;i<num;i++) {
			factorial=factorial*i;
			
		}
		System.out.println(factorial);
	}

}
